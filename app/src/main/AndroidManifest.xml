<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.myappartment.my_appartment">
    android:name="android.support.multidex.MultiDexApplication"
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Requests.requestsList"
            android:exported="true" />
        <activity android:name=".SignUp.rentingReg" />
        <activity
            android:name=".SignUp.renterReg"
            android:exported="true" />
        <activity
            android:name=".Requests.createRequest"
            android:exported="true" />
        <activity
            android:name=".payments.Showpayments"
            android:exported="true" />
        <activity
            android:name=".payments.Createpayment"
            android:exported="true" />
        <activity
            android:name=".payments.Monthpayment"
            android:exported="true" />

        <service android:name=".java.MyFirebaseMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>

    </application>

</manifest>